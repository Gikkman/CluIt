<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cluit.visual.application.view.tabs.PyramidTabController">
   <content>
      <AnchorPane>
         <children>
            <Group fx:id="group_wrapper" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <Button layoutX="8.0" layoutY="22.0" fx:id="button_clear" onAction="#clear_tab" text="Clear" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </AnchorPane>
   </content>
</ScrollPane>
